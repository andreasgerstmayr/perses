{
  "name": "@perses-dev/jaeger-ui-plugin",
  "version": "0.45.0",
  "description": "Tracing View plugin for Perses",
  "license": "Apache-2.0",
  "homepage": "https://github.com/perses/perses/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perses/perses.git"
  },
  "bugs": {
    "url": "https://github.com/perses/perses/issues"
  },
  "module": "dist/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "build": "concurrently \"npm:build:*\"",
    "build:cjs": "swc ./src -d dist/cjs --config-file ../.cjs.swcrc",
    "build:esm": "swc ./src -d dist --config-file ../.swcrc",
    "build:types": "tsc --project tsconfig.build.json",
    "build:css": "rsync -r --include '*/' --include '*.css' --exclude '*' src/ dist/",
    "type-check": "tsc --noEmit",
    "start": "concurrently -P \"npm:build:* -- {*}\" -- --watch",
    "test": "cross-env TZ=UTC jest",
    "test:watch": "cross-env TZ=UTC jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx"
  },
  "dependencies": {
    "@perses-dev/core": "0.45.0",
    "@perses-dev/plugin-system": "0.45.0",
    "lodash": "^4.17.21",
    "@ant-design/compatible": "^5.1.2",
    "@pyroscope/flamegraph": "0.21.4",
    "antd": "^5.10.2",
    "chance": "^1.0.10",
    "classnames": "^2.5.1",
    "combokeys": "^3.0.0",
    "copy-to-clipboard": "^3.1.0",
    "dayjs": "^1.11.9",
    "deep-freeze": "^0.0.1",
    "drange": "^2.0.0",
    "global": "^4.3.2",
    "history": "^4.6.3",
    "lru-memoize": "^1.1.0",
    "memoize-one": "^6.0.0",
    "query-string": "^9.0.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.0.1",
    "react-is": "^18.2.0",
    "react-json-view-lite": "1.4.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-actions": "2.6.5",
    "redux-first-history": "^5.2.0",
    "redux-form": "^8.3.10",
    "tween-functions": "^1.2.0",
    "u-basscss": "2.0.1"
  },
  "peerDependencies": {
    "@mui/material": "^5.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.11.0"
  },
  "files": [
    "dist",
    "plugin.json"
  ]
}
